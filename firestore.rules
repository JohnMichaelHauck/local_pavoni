rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
      allow write: if request.auth != null && request.auth.uid == userId;
      allow read: if true;
  	}
    match /census/{country} {
      allow write: if true;
      allow read: if true;
  	}
  }
}